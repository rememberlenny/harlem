<p id="notice"><%= notice %></p>

<h1>Listing Newsletters</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Message</th>
      <th>Schedules</th>
      <th>Photos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @newsletters.each do |newsletter| %>
      <tr>
        <td><%= newsletter.title %></td>
        <td><%= newsletter.message %></td>
        <td><%= newsletter.schedules %></td>
        <td><%= newsletter.photos %></td>
        <td><%= link_to 'Show', newsletter, :class => 'button tiny' %></td>
        <td><%= link_to 'Edit', edit_newsletter_path(newsletter), :class => 'button tiny' %></td>
        <td><%= link_to 'Destroy', newsletter, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'button tiny' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Newsletter', new_newsletter_path, :class => 'button' %>
